    "import threading\n",
    "import webbrowser\n",
    "from app import app\n",
    "\n",
    "# Run Flask in background\n",
    "def run_app():\n",
    "    app.run(port=3000, debug=False, use_reloader=False)\n",
    "\n",
    "threading.Thread(target=run_app).start()\n",
    "\n",
    "# Open website automatically\n",
    "webbrowser.open(\"http://127.0.0.1:3000\")"
   ]
